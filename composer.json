{
    "name": "aleksip/plugin-data-transform",
    "description": "Data Transform Plugin for Pattern Lab PHP",
    "keywords": ["pattern lab", "plugin", "drupal"],
    "homepage": "https://github.com/aleksip/plugin-data-transform",
    "license": "GPL-2.0-or-later",
    "type": "patternlab-plugin",
    "authors": [
        {
            "name": "Aleksi Peebles"
        }
    ],
    "support": {
        "issues": "https://github.com/aleksip/plugin-data-transform/issues",
        "source": "https://github.com/aleksip/plugin-data-transform/releases"
    },
    "autoload": {
        "psr-4": {
            "aleksip\\DataTransformPlugin\\": "src/aleksip/DataTransformPlugin/",
            "Drupal\\Component\\Render\\": "src/Drupal/Component/Render/",
            "Drupal\\Component\\Utility\\": "src/Drupal/Component/Utility/",
            "Drupal\\Core\\": "src/Drupal/Core/",
            "Drupal\\Core\\Template\\": "src/Drupal/Core/Template/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "pattern-lab/core": "^2.6.3",
        "drupal/core-render": "^8.7",
        "drupal/core-utility": "^8.7"
    },
    "extra": {
        "patternlab": {
            "config": {
                "plugins": {
                    "dataTransform": {
                        "enabled": true,
                        "verbose": false
                    }
                }
            }
        }
    }
}
